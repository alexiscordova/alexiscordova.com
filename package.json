    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.2",
    "open": "0.0.5",
    "path": "^0.12.7",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
