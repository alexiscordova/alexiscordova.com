{
  "name": "alexiscordova.com",
  "version": "4.0.0",
  "description": "The personal portfolio site of Alexis Córdova, Front-End UI/UX Developer",
  "main": "src/index.jsx",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel security-check serve:src lint:watch",
    "serve:src": "babel-node tools/server.js NODE_ENV=development",
    "serve:dist": "babel-node tools/server.js NODE_ENV=production",
    "lint": "esw webpack.config.* src server.js tools",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "clean:dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean:dist lint",
    "build": "babel-node tools/build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexiscordova/portfolio.git"
  },
